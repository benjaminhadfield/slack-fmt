{
  "name": "slack-formatter",
  "version": "0.0.1",
  "description": "A Slack bot for formatting messages",
  "author": "Ben Hadfield <benjohnhadfield@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/benjaminhadfield/slack-fmt.git"
  },
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "tsc:watch": "tsc -w",
    "build": "rm -rf dist ; npm run tsc",
    "prepublish": "npm run build",
    "deploy:dev": "npm run build && sls deploy -v",
    "deploy:prod": "npm run build && sls deploy -v --stage production",
    "test": "mocha dist/**/*spec.js"
  },
  "engines": {
    "node": "6.10"
  },
  "keywords": [
    "slack-bot",
    "node"
  ],
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "jsonic": "^0.3.0",
    "querystring": "^0.2.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.48",
    "babel-core": "^7.0.0-bridge.0",
    "chai": "^4.1.2",
    "mocha": "^5.0.4",
    "typescript": "^2.7.2"
  }
}
